<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="287.0" prefWidth="351.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetwork.gui.ControllerLogin">
    <children>
        <Label layoutX="34.0" layoutY="21.0" text="Login to Socialnetwork">
            <font>
                <Font name="System Bold" size="26.0" />
            </font></Label>
        <TextField fx:id="usernameTextField" layoutX="101.0" layoutY="72.0" promptText="Username" />
        <PasswordField fx:id="passwordTextField" layoutX="101.0" layoutY="118.0" promptText="Password" />
        <Button layoutX="149.0" layoutY="158.0" onAction="#onUserSignIn" text="Sign in" />
        <Label layoutX="118.0" layoutY="221.0" text="Create a new account" />
        <Button layoutX="147.0" layoutY="247.0" onAction="#onUserSignUp" text="Sign up" />
    </children>
</AnchorPane>
